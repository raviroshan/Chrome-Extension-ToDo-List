{
    "name": "To-Do",
    "short_name": "To-Do List",
    "manifest_version": 2,
    "version": "1.0.2",
    "description": "To-Do List",
    "offline_enabled": true,
    "icons": {
        "16": "images/icon.png",
        "48": "images/48.png",
        "128": "images/128.png"
    },
    "permissions": [
        "storage",
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_title": "To-Do List",
        "default_popup": "popup.html"
    }
}
